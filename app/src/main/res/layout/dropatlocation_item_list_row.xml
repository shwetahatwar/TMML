<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/pp_white"
    android:paddingStart="10dp"
    android:paddingTop="5dp"
    android:paddingEnd="10dp"
    android:paddingBottom="5dp">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/jobcardBarcodeHeadingId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:text="Job Card Id"
        android:textAlignment="viewStart"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/jobcardBarcodeItemTextId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintWidth_percent="50" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/jobcardBarcodeItemTextId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/lato"
        android:text="Job Card Id1"
        android:textAlignment="textEnd"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/trolleyBarcodeHeadingId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:text="Trolley Barcode"
        android:textAlignment="viewStart"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/trolleyBarcodeTextId"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/trolleyBarcodeTextId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/lato"
        android:text="Trolley Barcode1"
        android:textAlignment="textEnd"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/jobcardBarcodeItemTextId"
        app:layout_constraintTop_toBottomOf="@+id/jobcardBarcodeItemTextId" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/partNumberBarcodeHeadingId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lato"
        android:text="Part Number"
        android:textAlignment="viewStart"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/partNumberBarcodeTextId"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/partNumberBarcodeTextId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/lato"
        android:text="Part Number1"
        android:textAlignment="textEnd"
        android:textColor="@color/colorTextEntry"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/trolleyBarcodeTextId"
        app:layout_constraintTop_toBottomOf="@+id/trolleyBarcodeTextId" />

</androidx.constraintlayout.widget.ConstraintLayout>